
module all_4
{
  N = 10;

  function print_array(a[], N)
  {
   i=0;
   while(i < N) {
      ai=a[i];
	  io.print("a: ",ai);
	  io.println();
	  i=i+1;
   }
  }

  function main()
  {
   a=[N];


   i=0;
   while(i < N)
   {
   	a[i] = i;
	i=i+1;
   }

	 print_array(a, N);
  }
}
