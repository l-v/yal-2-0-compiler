
module wfv_1
{

 function zeros(a[])
 {
   i = 0;
   while(i < a.size) {
      a[i] = 0;
      i = i + 1;
   }
 }

 function t=pow(a[])
 {
   i = 0;
   t = 1;
   while(i < a.size) {
      t = t*a[i];
      i = i + 1;
   }
 }

 function main()
 {

   a = [3];
   zeros(a);

   t = pow(a);

   io.println("pow a: ",t); // result= 0

 }
}
