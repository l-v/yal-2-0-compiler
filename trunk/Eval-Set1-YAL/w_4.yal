module w_4
{

function main()
{

  N = 10;
  M = 5;

  i = 0;
  j = 0;

  iter = 0;

  while(i < N) {
     j = 0;
     while(j < M) {
       j = j+1;
       iter = iter+1;
     }
     i = i+1;
  }


 io.println(iter); // result= 50
 io.println(i); // result= 10
 io.println(j); // result= 5
}

}