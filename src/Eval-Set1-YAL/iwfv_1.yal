module iwfv_1
{

function max=maxarray(a[], N) {

	max = a[0];

	i=1;
	while(i < N) {
		if(max < a[i]) {
			max = a[i];
		}
		i=i+1;
	}

	io.println("max: ", max);
}

 function main() {
 		  a=[10];
 		  i=0;
		  while(i < 10) {
		     a[i]=i;
		     i=i+1;
          }
 		  max=maxarray(a, 10);   // result = 9
	io.println("max: ", max);
 }

}