module if_4 {

function m=max(a,b) {
		if(a > b) {
			m = a;
		} else {
			m = b;
		}
}

function m=min(a,b) {
		if(a > b) {
			m = b;
		} else {
			m = a;
		}
}

function main() {
  d = 1;
  e = 2;
  m1 = max(d, e);
  io.println("m1: ",m1); // 2

  m1 = min(d, e);
  io.println("m1: ",m1); // 1
}
}

